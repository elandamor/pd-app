    "localforage": "1.5.6",
    "no-scroll": "2.1.0",
    "numeral": "2.0.6",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "15.6.0",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-apollo": "2.1.0-beta.2",
    "react-autosize-textarea": "3.0.2",
    "react-dev-utils": "^5.0.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
